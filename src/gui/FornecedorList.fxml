                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="Nome" GridPane.columnIndex="1" />
                  <TextField fx:id="txtFindByNome" prefHeight="24.0" prefWidth="612.0" GridPane.columnIndex="2" />
                  <Button fx:id="btPesquisa" mnemonicParsing="false" onAction="#onBtPesquisaAction" text="Pesquisar" GridPane.columnIndex="4" />
               </children>
            </GridPane>
        </items>
      </ToolBar>
      <TableView fx:id="tableViewFornecedor" prefHeight="417.0" prefWidth="689.0">
        <columns>
            <TableColumn fx:id="tableColumnContato" prefWidth="75.0" text="Column X" visible="false" />
            <TableColumn fx:id="tableColumnEmail1" prefWidth="75.0" text="Column X" visible="false" />
            <TableColumn fx:id="tableColumnEmail2" prefWidth="75.0" text="Column X" visible="false" />
            <TableColumn fx:id="tableColumnFantasia" prefWidth="75.0" text="Column X" visible="false" />
          <TableColumn fx:id="tableColumnID" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="tableColumnName" prefWidth="400.0" text="Razao Social" />
            <TableColumn fx:id="tableColumnEDIT" prefWidth="100.0" />
            <TableColumn fx:id="tableColumnREMOVE" prefWidth="75.0" />
            <TableColumn fx:id="tableColumnFone1" prefWidth="75.0" text="Column X" visible="false" />
            <TableColumn fx:id="tableColumnFone2" prefWidth="75.0" text="Column X" visible="false" />
            <TableColumn fx:id="tableColumnBairro" prefWidth="75.0" text="Column X" visible="false" />
